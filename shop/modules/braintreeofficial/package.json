{
  "name": "braintreeofficial",
  "version": "1.0.0",
  "description": "Braintree official module",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "webpack --mode development --colors --devtool source-map --hide-modules -w",
    "build": "webpack --mode production --colors --devtool source-map --progress -p",
    "doc-build": "stylemark -i 202/doc -o 202/build/doc -c 202/doc/config.yml",
    "doc-watch": "stylemark -i 202/doc -o 202/build/doc -c 202/doc/config.yml -w -b",
    "lint-css": "stylelint \"_dev/scss/**/*.scss\" --cache --cache-location .cache/.stylelintcache",
    "lint-css-fix": "stylelint \"_dev/scss/**/*.scss\" --cache --cache-location .cache/.stylelintcache --fix",
    "lint-js": "eslint --cache --cache-location .cache/.eslintcache _dev/js/",
    "lint-js-fix": "eslint --cache --cache-location .cache/.eslintcache _dev/js/ --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/202-ecommerce/braintree.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/202-ecommerce/braintree/issues"
  },
  "homepage": "https://github.com/202-ecommerce/braintree#readme",
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "eslint": "^5.16.0",
    "webpack": "^4.34.0"
  },
  "dependencies": {
    "autoprefixer": "^9.6.0",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.0.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-html": "^5.0.5",
    "eslint-plugin-import": "^2.17.3",
    "mini-css-extract-plugin": "^0.7.0",
    "node-sass": "^4.12.0",
    "postcss-load-config": "^2.1.0",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^7.1.0",
    "stylelint": "^10.1.0",
    "stylelint-config-twbs-bootstrap": "^0.3.1",
    "stylelint-no-unsupported-browser-features": "^3.0.2",
    "stylemark": "^3.0.3",
    "terser-webpack-plugin": "^1.3.0",
    "webpack-cli": "^3.3.4",
    "webpack-fix-style-only-entries": "^0.3.0"
  },
  "browserslist": [
    "last 2 versions",
    "iOS >= 8",
    "> 1%",
    "Firefox ESR"
